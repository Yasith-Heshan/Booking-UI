{
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "paths": {
      "src": [
        "src"
      ]
    },
    "sourceMap": true,
    "strictNullChecks": false,
    "strict": true,
    "target": "ES2020",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ]
  },
}